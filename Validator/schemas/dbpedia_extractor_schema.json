{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"entities": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"type": {
						"type": "string"
					},
					"dbpedia": {
						"type": [
							"string",
							"null"
						],
						"format": "uri"
					}
				},
				"required": [
					"name",
					"type",
					"dbpedia"
				]
			}
		}
	},
	"required": [
		"entities"
	]
}